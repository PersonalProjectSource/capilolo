{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": {
            "": "src/",
            "DoctrineExtensions" : "lib/"
        }
    },
   
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",
        "symfony/icu": "1.0.*@dev",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "stof/doctrine-extensions-bundle": "~1.1@dev",
        "symfony/icu": "1.*",
        "jms/security-extra-bundle": "1.5.*",
        "phpoffice/phpexcel": "dev-master#e69a5e4d0ffa7fb6f171859e0a04346e580df30b",
        "lexik/translation-bundle": "~2.0",
        "noiselabs/nusoap-bundle": "dev-master#5f4057b77b3ce1799840d2aad651475f71afa415",
        "beberlei/DoctrineExtensions": "dev-master#5e4ec9c3ec3434151e1c73144b4ab87ae30aefbc",
        "friendsofsymfony/rest-bundle": "1.4.2",
        "jms/serializer-bundle": "0.13.*",
        "nelmio/api-doc-bundle": "2.7.0",
        "friendsofsymfony/oauth-server-bundle": "1.4.2",
        "snc/redis-bundle": "1.1.x-dev",
        "predis/predis": "0.8.x-dev",
        "friendsofsymfony/jsrouting-bundle": "@stable",
        "symfony/proxy-manager-bridge": "2.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "behat/mink": "~1.5",
        "behat/mink-extension": "~2.0",
        "behat/mink-browserkit-driver": "~1.1",
        "behat/mink-bundle": "~1.4",
        "behat/mink-selenium2-driver": "*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "nusphere/nusoap",
                "version": "0.9.5",
                "dist": {
                    "url": "http://downloads.sourceforge.net/project/nusoap/nusoap/0.9.5/nusoap-0.9.5.zip",
                    "type": "zip"
                },
                "autoload": {
                    "classmap": ["lib/"]
                }
            }
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "keep-outdated": true
        },
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
